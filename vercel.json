{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true",
    "NODE_ENV": "production"
  },
  "functions": {
    "index.js": {
      "memory": 3008,
      "maxDuration": 60
    }
  }
} 